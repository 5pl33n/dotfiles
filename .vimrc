set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set number relativenumber
set expandtab
set tabstop=2 shiftwidth=2 expandtab
set nowrap
set incsearch
set ignorecase
set showmatch
set hlsearch
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx

